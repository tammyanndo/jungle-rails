<tr>
  <td>
    <%= image_tag item.product.image.tiny %>
  </td>
  <td>
    <h4><%= item.product.name %></h4>
    <br>
  </td>
  <td>
    <%= humanized_money_with_symbol (item.item_price_cents) / 100.0 %>
  </td>
  <td>
    <%= item.quantity %>
  </td>
  <td>
    <%= humanized_money_with_symbol (item.item_price_cents / 100.0) * item.quantity %>
  </td>
</tr>
